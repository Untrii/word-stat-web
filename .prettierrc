{
  "useTabs": false,
  "singleQuote": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "semi": false,
  "trailingComma": "all",
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
